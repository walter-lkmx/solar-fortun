{
  "name": "palmerhq/presspack",
  "description": "Minimalist wordpress theme",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "wpackagist-plugin/members": "1.1.3",
    "wpackagist-plugin/woocommerce": "3.3.1",
    "wpackagist-plugin/w3-total-cache": "0.9.6",
    "wpackagist-plugin/wp-config-file-editor": "1.6.7",
    "wpackagist-plugin/conekta-payment-gateway": "3.0.4",
    "roots/soil": "4.1.0" 
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
